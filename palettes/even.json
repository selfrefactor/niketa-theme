{
	"name": "_Palette",
	"type": "light",
	"colors": {
		"editor.background": "COLOR_BACK",
		"activityBar.background": "COLOR_SECONDARY",
		"editor.selectionBackground": "COLOR_SELECTION",
		"editor.lineHighlightBackground": "COLOR_SECONDARY",
		"editorBracketMatch.background": "COLOR_5_DARKER",
		"editorBracketMatch.border": "COLOR_4_DARKER",
		"editorGroupHeader.tabsBackground":"COLOR_BACK_DARK",
		"editorGutter.background": "COLOR_BACK",
		"editorLineNumber.foreground": "COLOR_SECONDARY_DARKER",
		"scrollbarSlider.background": "COLOR_SECONDARY",
		"scrollbarSlider.hoverBackground": "COLOR_SECONDARY_DARKER",
		"sideBar.background": "COLOR_SECONDARY_DARK",
		"statusBar.background": "COLOR_SECONDARY_DARKEST", 
		"tab.inactiveForeground":"COLOR_1_LIGHTER",
		"tab.inactiveBackground":"COLOR_SECONDARY_DARKEST",
		"tab.activeForeground":"COLOR_1_DARK",
		"tab.activeBackground":"COLOR_BACK",
		"tab.border": "COLOR_BACK"
	},
	"tokenColors": [
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "COLOR_5_LIGHT",
				"fontStyle": "bold"
			}
		},
		{
			"SK":"const let for",
			"name": "Storage types",
			"scope": "storage.type",
			"settings": {
				"foreground": "COLOR_0_DARK"
			}
		},
		{
			"scope": "entity.name.function.method",
			"settings": {
				"foreground": "COLOR_3"
			}
		},
		{
			"SK": "function SK",
			"scope": "entity.name.function",
			"settings": {
				"fontStyle": "underline",
				"foreground": "COLOR_1_LIGHT"
			}
		},
		{
			"SK": "function SK",
			"scope": "support.function.dom.js",
			"settings": {
				"fontStyle": "underline",
				"foreground": "COLOR_0_DARKER"
			}
		},
		{
			"name": "Numbers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "COLOR_5"
			}
		},
		{
			"name": "Built-in constants",
			"scope": "constant.language",
			"settings": {
				"fontStyle": "bold",
				"foreground": "COLOR_4_LIGHT"
			}
		},
		{
			"name": "Function arguments",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "bold",
				"foreground": "COLOR_0_LIGHTER"
			}
		},
		{
			"name": "Library functions",
			"scope": "support.function",
			"settings": {
				"foreground": "COLOR_2"
			}
		},
		{
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"foreground": "COLOR_1_DARKER"
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "COLOR_5_DARK",
				"fontStyle":"bold"
			}
		},
		{
			"name": "User-defined constants",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "COLOR_4_DARKER"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "COLOR_5_DARKER"
			}
		},
		{
			"scope": "variable.other.readwrite",
			"settings": {
				"foreground": "COLOR_3_DARKER"
			}
		},
		{
			"scope": "variable.other.readwrite.js,variable.other.readwrite.ts,variable.other.readwrite.tsx,variable.other.object.js,punctuation.accessor.js",
			"settings": {
				"foreground": "COLOR_4"
			}
		},
		{
			"scope": "variable.other.property.js",
			"settings": {
				"foreground": "COLOR_3"
			}
		},
		{
			"scope": "keyword.control.at-rule.import.scss,keyword.control.module.js,keyword.operator.accessor",
			"settings": {
				"foreground": "COLOR_5"
			}
		},
		{
			"SK":"IMPORT r FROM 'rambda'",
			"scope": "keyword.control.import.js,keyword.control.from.js,keyword.control.export.js,keyword.control.default.js,support.type.object.module.ts",
			"settings": {
				"foreground": "COLOR_5"
			}
		},
		{
			"scope": "string.template",
			"settings": {
				"foreground": "COLOR_5"
			}
		},
		{
			"scope": "string.quoted.single.js,string.quoted.single.scss,string.quoted.single.attribute-value.scss,string.quoted.single.json,string.quoted.single.ts",
			"settings": {
				"foreground": "COLOR_2"
			}
		},
		{
			"scope": "string.quoted.double.js,string.quoted.double.scss,string.quoted.double.attribute-value.scss,string.quoted.double.json,string.quoted.double.ts",
			"settings": {
				"foreground": "COLOR_0_DARKER"
			}
		},
		{
			"scope": "constant.other.object.key.js,meta.object-literal.key.js,support.type.property-name.json,meta.object-literal.key.ts,variable.other.property.ts",
			"settings": {
				"foreground": "COLOR_4_DARKER"
			}
		},
		{
			"scope": "support.class.builtin.js",
			"settings": {
				"foreground": "COLOR_5_DARK"
			}
		},

		{
			"scope": "comment",
			"settings": {
				"foreground": "COLOR_5"
			}
		},
		{
			"scope": "comment.block.documentation",
			"settings": {
				"foreground": "COLOR_4"
			}
		},
		{
			"scope": "comment.line.double-slash,punctuation.definition.comment.scss,comment.line.scss",
			"settings": {
				"foreground": "COLOR_5_DARKER"
			}
		},
		{
			"scope": "punctuation.definition.comment.html",
			"settings": {
				"foreground": "COLOR_2"
			}
		},
		{
			"scope": "comment.block.html",
			"settings": {
				"foreground": "COLOR_SECONDARY_DARKER"
			}
		},
		{
			"scope": "variable.language.this,storage.type.function.js,keyword.control.flow.js,support.variable.dom.js,support.variable.property.dom.js,support.function.promise.ts,keyword.control.flow.ts",
			"settings": {
				"foreground": "COLOR_0"
			}
		},
		{
			"scope": "constant.language.null.js",
			"settings": {
				"fontStyle":	"bold",
				"foreground": "COLOR_3_DARK"
			}
		},
		{
			"scope": "constant.language.boolean,constant.language.boolean.false.ts,constant.language.boolean.true.ts",
			"settings": {
				"fontStyle":	"bold",
				"foreground": "COLOR_1"
			}
		},
		{
			"scope": "support.type.object.console.js",
			"settings": {
				"foreground": "#d33682"
			}
		},
		{
			"scope": "support.function.console.js",
			"settings": {
				"foreground": "#d33682"
			}
		},
		{
			"scope": "variable.other.class.js",
			"settings": {
				"foreground": "COLOR_3_DARK"
			}
		},
		{
			"name": "Tag attributes",
			"scope": "entity.other.attribute-name,entity.other.attribute-name.class.css",
			"settings": {
				"foreground": "#268bd2"
			}
		},
		{
			"scope": "support.type.property-name.scss,support.type.property-name.css",
			"settings": {
				"fontStyle": "italic",
				"foreground": "COLOR_3"
			}
		},
		{
			"scope": "support.constant.property-value.scss,support.constant.property-value.css",
			"settings": {
				"foreground": "#3C8F93"
			}
		},
		{
			"scope": "entity.name.tag.scss,support.constant.tag.css",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "support.function.misc.scss",
			"settings": {
				"foreground": "#2c91af"
			}
		},
		{
			"scope": "variable.scss",
			"settings": {
				"foreground": "#00AFD6"
			}
		},
		{
			"scope": "variable.language.constructor",
			"settings": {
				"foreground": "COLOR_0_LIGHT",
				"fontStyle": "underline"
			}
		},
		{
			"settings": {
				"background": "#000c18",
				"foreground": "#6688cc"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "COLOR_2_DARKER"
			}
		},
		{
			"scope": "storage.type.let",
			"settings": {
				"foreground": "#d33682"
			}
		},
		{
			"name": "Class names",
			"scope": [
				"entity.name.class",
				"entity.name.type"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "COLOR_1_DARK"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "underline",
				"foreground": "COLOR_4_LIGHTER"
			}
		},
		{
			"name": "Tag names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "COLOR_0_DARKER"
			}
		},
		{
			"name": "diff: header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#E0EDDD"
			}
		},
		{
			"name": "diff: deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#dc322f"
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#cb4b16"
			}
		},
		{
			"name": "diff: inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#219186"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#22aa44"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#22aa44"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#9966b8"
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"foreground": "#ddbb88"
			}
		}
	]
}