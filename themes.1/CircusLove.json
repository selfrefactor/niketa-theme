{
  "name": "CircusLove",
  "type": "light",
  "colors": {
    "editor.background": "#f9f6ec",
    "activityBar.background": "#e8e7e0",
    "editor.selectionBackground": "#ffb1b8",
    "editor.lineHighlightBackground": "#e8e7e0",
    "editorBracketMatch.background": "#3D7FA4",
    "editorBracketMatch.border": "#08565B",
    "editorGroupHeader.tabsBackground": "#F6F2E3",
    "editorGutter.background": "#f9f6ec",
    "editorLineNumber.foreground": "#DEDDD3",
    "scrollbarSlider.background": "#e8e7e0",
    "scrollbarSlider.hoverBackground": "#DEDDD3",
    "sideBar.background": "#E3E2D9",
    "statusBar.background": "#bdc3c7",
    "tab.activeForeground": "#C46493",
    "tab.activeBackground": "#DEDDD3",
    "tab.inactiveForeground": "#C46493",
    "tab.inactiveBackground": "#ede6ec",
    "tab.border": "#E3E2D9"
  },
  "tokenColors": [
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#4289B1",
        "fontStyle": "bold"
      }
    },
    {
      "SK": "const let for",
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#892039"
      }
    },
    {
      "scope": "entity.name.function.method",
      "settings": {
        "foreground": "#588859"
      }
    },
    {
      "SK": "function SK",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#C46493"
      }
    },
    {
      "SK": "function SK",
      "scope": "support.function.dom.js",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#861F37"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#4086ad"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#085D62"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#94233D"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#68355d"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#BE5588"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#3E83A9",
        "fontStyle": "bold"
      }
    },
    {
      "name": "User-defined constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#08565B"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#3D7FA4"
      }
    },
    {
      "scope": "variable.other.readwrite",
      "settings": {
        "foreground": "#548155"
      }
    },
    {
      "scope": "variable.other.readwrite.js,variable.other.readwrite.ts,variable.other.readwrite.tsx,variable.other.object.js,punctuation.accessor.js",
      "settings": {
        "foreground": "#085b60"
      }
    },
    {
      "scope": "variable.other.property.js",
      "settings": {
        "foreground": "#588859"
      }
    },
    {
      "scope": "keyword.control.at-rule.import.scss,keyword.control.module.js,keyword.operator.accessor",
      "settings": {
        "foreground": "#4086ad"
      }
    },
    {
      "SK": "IMPORT r FROM 'rambda'",
      "scope": "keyword.control.import.js,keyword.control.from.js,keyword.control.export.js,keyword.control.default.js,support.type.object.module.ts",
      "settings": {
        "foreground": "#4086ad"
      }
    },
    {
      "scope": "string.template",
      "settings": {
        "foreground": "#4086ad"
      }
    },
    {
      "scope": "string.quoted.single.js,string.quoted.single.scss,string.quoted.single.attribute-value.scss,string.quoted.single.json,string.quoted.single.ts",
      "settings": {
        "foreground": "#68355d"
      }
