{
  "name": "BraveWhisky",
  "type": "light",
  "colors": {
    "editor.background": "#f2ede0",
    "activityBar.background": "#e3e4d7",
    "editor.selectionBackground": "#ffe0e0",
    "editor.lineHighlightBackground": "#e3e4d7",
    "editorBracketMatch.background": "#55978B",
    "editorBracketMatch.border": "#7C3939",
    "editorGroupHeader.tabsBackground": "#E6DCC2",
    "editorGutter.background": "#f2ede0",
    "editorLineNumber.foreground": "#C2C4A7",
    "scrollbarSlider.background": "#e3e4d7",
    "scrollbarSlider.hoverBackground": "#C2C4A7",
    "sideBar.background": "#D2D4BF",
    "statusBar.background": "#D2D4BF",
    "tab.activeForeground": "#4E5F52",
    "tab.activeBackground": "#C2C4A7",
    "tab.inactiveForeground": "#4E5F52",
    "tab.inactiveBackground": "#D2D4BF",
    "tab.border": "#D2D4BF"
  },
  "tokenColors": [
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#974545",
        "fontStyle": "bold"
      }
    },
    {
      "SK": "const let for",
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#ca434d"
      }
    },
    {
      "scope": "entity.name.function.method",
      "settings": {
        "foreground": "#547e80"
      }
    },
    {
      "SK": "function SK",
      "scope": "entity.name.function,support.function.dom.js",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#4E5F52"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#82B8AF"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#547e80"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#BF3640"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#974545"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#547e80"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#48574b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "User-defined constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#547e80"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#71afa4"
      }
    },
    {
      "scope": "variable.other.readwrite,variable.other.readwrite.js,variable.other.readwrite.ts,variable.other.readwrite.tsx,variable.other.object.js,punctuation.accessor.js",
      "settings": {
        "foreground": "#877b9e"
      }
    },
    {
      "scope": "variable.other.property.js",
      "settings": {
        "foreground": "#6E6285"
      }
    },
    {
      "scope": "keyword.control.at-rule.import.scss,keyword.control.module.js,keyword.operator.accessor",
      "settings": {
        "foreground": "#71afa4"
      }
    },
    {
      "SK": "IMPORT r FROM 'rambda'",
      "scope": "keyword.control.import.js,keyword.control.from.js,keyword.control.export.js,keyword.control.default.js,support.type.object.module.ts",
      "settings": {
        "foreground": "#71afa4"
      }
    },
    {
      "scope": "string.template",
      "settings": {
        "foreground": "#639597"
      }
    },
    {
      "scope": "string.quoted.single.js,string.quoted.single.scss,string.quoted.single.attribute-value.scss,string.quoted.single.json,string.quoted.single.ts",
      "settings": {
        "foreground": "#639597"
      }
    },
    {
      "scope": "string.quoted.double.js,string.quoted.double.scss,string.quoted.double.attribute-value.scss,string.quoted.double.json,string.quoted.double.ts",
      "settings": {
        "foreground": "#639597"
      }
