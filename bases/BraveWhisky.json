        "foreground": "#7C3939"
      }
    },
    {
      "scope": "support.class.builtin.js",
      "settings": {
        "foreground": "#4C7374"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#55978B"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#82B8AF"
      }
    },
    {
      "scope": "comment.line.double-slash,punctuation.definition.comment.scss,comment.line.scss",
      "settings": {
        "foreground": "#55978B"
      }
    },
    {
      "scope": "punctuation.definition.comment.html",
      "settings": {
        "foreground": "#877b9e"
      }
    },
    {
      "scope": "comment.block.html",
      "settings": {
        "foreground": "#C2C4A7"
      }
    },
    {
      "scope": "variable.language.this,storage.type.function.js,keyword.control.flow.js,support.variable.dom.js,support.variable.property.dom.js,support.function.promise.ts,keyword.control.flow.ts",
      "settings": {
        "foreground": "#547e80"
      }
    },
    {
      "scope": "constant.language.null.js",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#4C7374"
      }
    },
    {
      "scope": "constant.language.boolean,constant.language.boolean.false.ts,constant.language.boolean.true.ts",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#AC313A"
      }
    },
    {
      "scope": "support.type.object.console.js",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "support.function.console.js",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "variable.other.class.js",
      "settings": {
        "foreground": "#4C7374"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name,entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "scope": "support.type.property-name.scss,support.type.property-name.css",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#547e80"
      }
    },
    {
      "scope": "support.constant.property-value.scss,support.constant.property-value.css",
      "settings": {
        "foreground": "#3C8F93"
      }
    },
    {
      "scope": "entity.name.tag.scss,support.constant.tag.css",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "support.function.misc.scss",
      "settings": {
        "foreground": "#2c91af"
      }
    },
    {
      "scope": "variable.scss",
      "settings": {
        "foreground": "#00AFD6"
      }
    },
    {
      "scope": "variable.language.constructor",
      "settings": {
        "foreground": "#456769"
      }
    },
    {
      "settings": {
        "background": "#000c18",
        "foreground": "#6688cc"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#6E6285"
      }
    },
    {
      "scope": "storage.type.let",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "name": "Class names",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "fontStyle": "undereline",
        "foreground": "#893F3F"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#B05353"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#6E6285"
      }
    },
    {
      "name": "diff: header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#ddbb88"
      }
    }
  ]
}