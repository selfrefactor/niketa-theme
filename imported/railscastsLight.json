{
  "name": "NiketaRailscastsLight",
  "type": "light",
  "tokenColors": [
    {
      "settings": {
        "background": "#f9f7f3",
        "foreground": "#3a4055"
      }
    },
    {
      "name": "Text",
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#3a4055"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#d4cfc9"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.definition.string",
        "punctuation.definition.variable",
        "punctuation.definition.string",
        "punctuation.definition.parameters",
        "punctuation.definition.string",
        "punctuation.definition.array"
      ],
      "settings": {
        "foreground": "#3a4055"
      }
    },
    {
      "name": "Delimiters",
      "scope": "none",
      "settings": {
        "foreground": "#3a4055"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#3a4055"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#b6b3eb"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#da4939"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.require",
        "support.function.any-method"
      ],
      "settings": {
        "foreground": "#6d9cbe"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "support.class",
        "entity.name.class",
        "entity.name.type.class"
      ],
      "settings": {
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Classes",
      "scope": "meta.class",
      "settings": {
        "foreground": "#272935"
      }
    },
    {
      "name": "Methods",
      "scope": "keyword.other.special-method",
      "settings": {
        "foreground": "#6d9cbe"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#b6b3eb"
      }
    },
    {
      "name": "Support",
      "scope": "support.function",
      "settings": {
        "foreground": "#519f50"
      }
    },
    {
      "name": "Strings, Inherited Class",
      "scope": [
        "string",
        "constant.other.symbol",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#a5c261"
      }
    },
    {
      "name": "Integers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Floats",
      "scope": "none",
      "settings": {
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Boolean",
      "scope": "none",
      "settings": {
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Constants",
      "scope": "constant",
      "settings": {
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#da4939"
      }
    },
    {
      "name": "Attributes",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Attribute IDs",
      "scope": [
        "entity.other.attribute-name.id",
        "punctuation.definition.entity"
      ],
      "settings": {
        "foreground": "#6d9cbe"
      }
    },
    {
      "name": "Selector",
      "scope": "meta.selector",
      "settings": {
        "foreground": "#b6b3eb"
      }
    },
    {
      "name": "Values",
      "scope": "none",
      "settings": {
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Headings",
      "scope": [
        "markup.heading punctuation.definition.heading",
        "entity.name.section"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#6d9cbe"
      }
    },
    {
      "name": "Units",
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "markup.bold",
        "punctuation.definition.bold"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "markup.italic",
        "punctuation.definition.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b6b3eb"
      }
    },
    {
      "name": "Code",
      "scope": "markup.raw.inline",
      "settings": {
        "foreground": "#a5c261"
      }
    },
    {
      "name": "Link Text",
      "scope": "string.other.link",
      "settings": {
        "foreground": "#da4939"
      }
    },
    {
      "name": "Link Url",
      "scope": "meta.link",
      "settings": {
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#da4939"
      }
    },
    {
      "name": "Quotes",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#cc7833"
      }
    },
    {
      "name": "Separator",
      "scope": "meta.separator",
      "settings": {
        "background": "#f4f1ed",
        "foreground": "#3a4055"
      }
    },
    {
      "name": "Inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#a5c261"
      }
    },
    {
      "name": "Deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#da4939"
      }
    },
    {
      "name": "Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#b6b3eb"
      }
    },
    {
      "name": "Colors",
      "scope": "constant.other.color",
      "settings": {
        "foreground": "#519f50"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#519f50"
      }
    },
    {
      "name": "Escape Characters",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#519f50"
      }
    },
    {
      "name": "Embedded",
      "scope": [
        "punctuation.section.embedded",
        "variable.interpolation"
      ],
      "settings": {
        "foreground": "#bc9458"
      }
    },
    {
      "name": "Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "background": "#da4939",
        "foreground": "#f9f7f3"
      }
    },
    {
      "name": "Broken",
      "scope": "invalid.broken",
      "settings": {
        "background": "#cc7833",
        "foreground": "#2b2b2b"
      }
    },
    {
      "name": "Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "#bc9458",
        "foreground": "#f9f7f3"
      }
    },
    {
      "name": "Unimplemented",
      "scope": "invalid.unimplemented",
      "settings": {
        "background": "#e6e1dc",
        "foreground": "#2b2b2b"
      }
    }
  ],
  "colors": {
    "editor.background": "#f9f7f3",
    "editorCursor.foreground": "#3a4055",
    "editor.foreground": "#3a4055",
    "editorWhitespace.foreground": "#f4f1ed",
    "editor.lineHighlightBackground": "#d4cfc955",
    "editor.selectionBackground": "#f4f1ed"
  }
}
