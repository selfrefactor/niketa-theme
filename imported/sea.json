        "meta.separator"
      ],
      "settings": {
        "fontStyle": "bold",
        "background": "#EFF1F5",
        "foreground": "#4f5b66"
      }
    },
    {
      "name": "Markup - Table",
      "scope": [
        "markup.table"
      ],
      "settings": {
        "foreground": "#4e5a65"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#374451"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#866117"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#6F2D33"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#698199"
      }
