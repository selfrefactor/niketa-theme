    {
      "name": "Markup - Raw Block",
      "scope": [
        "markup.raw.block"
      ],
      "settings": {
        "foreground": "#3E4851"
      }
    },
    {
      "name": "Markdown - Raw",
      "scope": [
        "markup.raw.block.fenced.markdown",
        "markup.inline.raw.string.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#3E4851"
      }
    },
    {
      "name": "Markdown - Separator",
