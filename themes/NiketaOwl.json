{
  "name": "AdvancedOwl",
  "type": "light",
  "colors": {
    "editor.background": "#d8d5c9",
    "activityBar.background": "#bac2c0",
    "editor.selectionBackground": "#ffa5bd",
    "editor.lineHighlightBackground": "#bac2c0",
    "editorBracketMatch.background": "#275968",
    "editorBracketMatch.border": "#AB3574",
    "editorGroupHeader.tabsBackground": "#D1CDBF",
    "editorGutter.background": "#d8d5c9",
    "editorLineNumber.foreground": "#AAB4B1",
    "scrollbarSlider.background": "#bac2c0",
    "scrollbarSlider.hoverBackground": "#AAB4B1",
    "sideBar.background": "#B2BBB9",
    "statusBar.background": "#A2ADAA",
    "tab.inactiveForeground": "#e7f7f7",
    "tab.inactiveBackground": "#A2ADAA",
    "tab.activeForeground": "#C55E33",
    "tab.activeBackground": "#bac2c0",
    "tab.border": "#B2BBB9"
  },
  "tokenColors": [
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#2C6576",
        "fontStyle": "bold"
      }
    },
    {
      "SK": "const let for",
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#776349"
      }
    },
    {
      "scope": "entity.name.function.method",
      "settings": {
        "foreground": "#27ae60"
      }
    },
    {
      "SK": "function SK",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#C55E33"
      }
    },
    {
      "SK": "function SK",
      "scope": "support.function.dom.js",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#725F46"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#2a6171"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#C13C83"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#866F52"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#218aaf"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#AE532D"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#285D6C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "User-defined constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#AB3574"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#275968"
      }
    },
    {
      "scope": "variable.other.readwrite",
      "settings": {
        "foreground": "#24A058"
      }
    },
    {
      "scope": "variable.other.readwrite.js,variable.other.readwrite.ts,variable.other.readwrite.tsx,variable.other.object.js,punctuation.accessor.js",
      "settings": {
        "foreground": "#ba3a7e"
      }
    },
    {
      "scope": "variable.other.property.js",
      "settings": {
        "foreground": "#27ae60"
      }
    },
    {
      "scope": "keyword.control.at-rule.import.scss,keyword.control.module.js,keyword.operator.accessor",
      "settings": {
        "foreground": "#2a6171"
      }
    },
    {
      "SK": "IMPORT r FROM 'rambda'",
      "scope": "keyword.control.import.js,keyword.control.from.js,keyword.control.export.js,keyword.control.default.js,support.type.object.module.ts",
      "settings": {
        "foreground": "#2a6171"
      }
    },
    {
      "scope": "string.template",
      "settings": {
        "foreground": "#2a6171"
      }
    },
    {
      "scope": "string.quoted.single.js,string.quoted.single.scss,string.quoted.single.attribute-value.scss,string.quoted.single.json,string.quoted.single.ts",
      "settings": {
        "foreground": "#218aaf"
      }
    },
    {
      "scope": "string.quoted.double.js,string.quoted.double.scss,string.quoted.double.attribute-value.scss,string.quoted.double.json,string.quoted.double.ts",
      "settings": {
        "foreground": "#725F46"
      }
    },
    {
      "scope": "constant.other.object.key.js,meta.object-literal.key.js,support.type.property-name.json,meta.object-literal.key.ts,variable.other.property.ts",
      "settings": {
        "foreground": "#AB3574"
      }
    },
    {
      "scope": "support.class.builtin.js",
      "settings": {
        "foreground": "#285D6C"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#2a6171"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#ba3a7e"
      }
    },
    {
      "scope": "comment.line.double-slash,punctuation.definition.comment.scss,comment.line.scss",
      "settings": {
        "foreground": "#275968"
      }
    },
    {
      "scope": "punctuation.definition.comment.html",
      "settings": {
        "foreground": "#218aaf"
      }
    },
    {
      "scope": "comment.block.html",
      "settings": {
        "foreground": "#AAB4B1"
      }
    },
    {
      "scope": "variable.language.this,storage.type.function.js,keyword.control.flow.js,support.variable.dom.js,support.variable.property.dom.js,support.function.promise.ts,keyword.control.flow.ts",
      "settings": {
        "foreground": "#7c674c"
      }
    },
    {
      "scope": "constant.language.null.js",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#25A75C"
      }
    },
    {
      "scope": "constant.language.boolean,constant.language.boolean.false.ts,constant.language.boolean.true.ts",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bd5a31"
      }
    },
    {
      "scope": "support.type.object.console.js",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "support.function.console.js",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "variable.other.class.js",
      "settings": {
        "foreground": "#25A75C"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name,entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "scope": "support.type.property-name.scss,support.type.property-name.css",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#27ae60"
      }
    },
    {
      "scope": "support.constant.property-value.scss,support.constant.property-value.css",
      "settings": {
        "foreground": "#3C8F93"
      }
    },
    {
      "scope": "entity.name.tag.scss,support.constant.tag.css",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "support.function.misc.scss",
      "settings": {
        "foreground": "#2c91af"
      }
    },
    {
      "scope": "variable.scss",
      "settings": {
        "foreground": "#00AFD6"
      }
    },
    {
      "scope": "variable.language.constructor",
      "settings": {
        "foreground": "#816B4F",
        "fontStyle": "underline"
      }
    },
    {
      "settings": {
        "background": "#000c18",
        "foreground": "#6688cc"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#1E7FA1"
      }
    },
    {
      "scope": "storage.type.let",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "name": "Class names",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#B5562F"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#C44388"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#725F46"
      }
    },
    {
      "name": "diff: header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#ddbb88"
      }
    }
  ]
}