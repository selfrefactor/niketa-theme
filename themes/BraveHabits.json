{
  "name": "BraveHabits",
  "type": "light",
  "colors": {
    "editor.background": "#f9f6f1",
    "activityBar.background": "#e6ddc3",
    "editor.selectionBackground": "#ffcae0",
    "editor.lineHighlightBackground": "#e6ddc359",
    "editorBracketMatch.background": "#3DAAA5",
    "editorBracketMatch.border": "#5E2D53",
    "editorGroupHeader.tabsBackground": "#F2EBE0",
    "editorGutter.background": "#f9f6f1",
    "editorLineNumber.foreground": "#DACCA5",
    "scrollbarSlider.background": "#e6ddc3",
    "scrollbarSlider.hoverBackground": "#DACCA5",
    "sideBar.background": "#E0D4B4",
    "statusBar.background": "#E0D4B4",
    "tab.inactiveForeground": "#5E2D53",
    "tab.inactiveBackground": "#c2aa4d",
    "tab.activeForeground": "#C67990",
    "tab.activeBackground": "#f9f6f1",
    "tab.border": "#f9f6f1"
  },
  "tokenColors": [
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#68325c",
        "fontStyle": "bold"
      }
    },
    {
      "SK": "const let for",
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#6a3951"
      }
    },
    {
      "scope": "entity.name.function.method",
      "settings": {
        "foreground": "#3b6160"
      }
    },
    {
      "SK": "function SK",
      "scope": "entity.name.function,support.function.dom.js",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#C67990"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#4EBFBB"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3b6160"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#65364D"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#68325c"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#3b6160"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#c16e87",
        "fontStyle": "bold"
      }
    },
    {
      "name": "User-defined constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#3b6160"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#45bcb7"
      }
    },
    {
      "scope": "variable.other.readwrite,variable.other.readwrite.js,variable.other.readwrite.ts,variable.other.readwrite.tsx,variable.other.object.js,punctuation.accessor.js",
      "settings": {
        "foreground": "#d7884f"
      }
    },
    {
      "scope": "variable.other.property.js",
      "settings": {
        "foreground": "#D27837"
      }
    },
    {
      "scope": "keyword.control.at-rule.import.scss,keyword.control.module.js,keyword.operator.accessor",
      "settings": {
        "foreground": "#45bcb7"
      }
    },
    {
