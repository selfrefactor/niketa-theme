        "foreground": "#3b6160"
      }
    },
    {
      "scope": "constant.language.null.js",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#385C5B"
      }
    },
    {
      "scope": "constant.language.boolean,constant.language.boolean.false.ts,constant.language.boolean.true.ts",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#5F3349"
      }
    },
    {
      "scope": "support.type.object.console.js",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "support.function.console.js",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "variable.other.class.js",
      "settings": {
        "foreground": "#385C5B"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name,entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "scope": "support.type.property-name.scss,support.type.property-name.css",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#3b6160"
      }
    },
    {
      "scope": "support.constant.property-value.scss,support.constant.property-value.css",
      "settings": {
        "foreground": "#3C8F93"
      }
    },
    {
      "scope": "entity.name.tag.scss,support.constant.tag.css",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "support.function.misc.scss",
      "settings": {
        "foreground": "#2c91af"
      }
    },
    {
      "scope": "variable.scss",
      "settings": {
        "foreground": "#00AFD6"
      }
    },
    {
      "scope": "variable.language.constructor",
      "settings": {
        "foreground": "#355756"
      }
    },
    {
      "settings": {
        "background": "#000c18",
        "foreground": "#6688cc"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#D27837"
      }
    },
    {
      "scope": "storage.type.let",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "name": "Class names",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "fontStyle": "undereline",
        "foreground": "#633057"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#723765"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#D27837"
      }
    },
    {
      "name": "diff: header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#ddbb88"
      }
    }
  ]
