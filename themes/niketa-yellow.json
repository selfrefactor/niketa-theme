{
	"name": "NiketaYellow",
	"type": "light",
	"colors": {
		"editor.foreground": "#6688cc",
		"activityBar.background": "#cfd5dd",
		"editor.background": "#EEDEBC",
		"editor.selectionBackground": "#D9E3C8",
		"editor.lineHighlightBackground": "#cfd5dd",
		"editorBracketMatch.background": "#2084d0",
		"editorBracketMatch.border": "#440b0b",
		"editorGroupHeader.tabsBackground":"#E3D9BB",
		"editorGutter.background": "#EEDEBC",
		"editorLineNumber.foreground": "#c24f4f74",
		"scrollbarSlider.background": "#cfd5dd",
		"scrollbarSlider.hoverBackground": "#979788",
		"sideBar.background": "#cfd5dd",
		"statusBar.background": "#cfd5dd", 
		"tab.activeForeground":"#440b0b",
		"tab.border": "#cfd5dd",
		"tab.inactiveBackground":"#cfd5dd",
		"tab.inactiveForeground":"#440b0b95"
	},
	"tokenColors": [
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#999999"
			}
		},
		{
			"scope": "variable.other.readwrite,variable.other.readwrite.js,variable.other.readwrite.ts,variable.other.readwrite.tsx,variable.other.object.js,punctuation.accessor.js",
			"settings": {
				"foreground": "#cd4e44"
			}
		},
		{
			"scope": "variable.other.property.js",
			"settings": {
				"foreground": "#00AFD6"
			}
		},
		{
			"scope": "keyword.control.at-rule.import.scss,keyword.control.module.js,keyword.operator.accessor",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "keyword.control.import.js,keyword.control.from.js,keyword.control.export.js,keyword.control.default.js,support.type.object.module.ts",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "entity.name.function.method",
			"settings": {
				"foreground": "#d33682"
			}
		},
		{
			"scope": "entity.name.function,support.function.dom.js",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#2c91af"
			}
		},
		{
			"scope": "string.template",
			"settings": {
				"foreground": "#22aa44"
			}
		},
		{
			"scope": "string.quoted.single.js,string.quoted.single.scss,string.quoted.single.attribute-value.scss,string.quoted.single.json,string.quoted.single.ts",
			"settings": {
				"foreground": "#22aa44"
			}
		},
		{
			"scope": "string.quoted.double.js,string.quoted.double.scss,string.quoted.double.attribute-value.scss,string.quoted.double.json,string.quoted.double.ts",
			"settings": {
				"foreground": "#22aa44"
			}
		},
		{
			"scope": "constant.other.object.key.js,meta.object-literal.key.js,support.type.property-name.json,meta.object-literal.key.ts,variable.other.property.ts",
			"settings": {
				"foreground": "#fe7d9d"
			}
		},
		{
			"scope": "support.class.builtin.js",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#555555"
			}
		},

		{
			"scope": "comment",
			"settings": {
				"foreground": "#384887"
			}
		},
		{
			"scope": "comment.block.documentation",
			"settings": {
				"foreground": "#384887"
			}
		},
		{
			"scope": "comment.line.double-slash,punctuation.definition.comment.scss,comment.line.scss",
			"settings": {
				"foreground": "#999999"
			}
		},
		{
			"scope": "punctuation.definition.comment.html",
			"settings": {
				"foreground": "#999999"
			}
		},
		{
			"scope": "comment.block.html",
			"settings": {
				"foreground": "#384887"
			}
		},
		{
			"scope": "variable.language.this,storage.type.function.js,keyword.control.flow.js,support.variable.dom.js,support.variable.property.dom.js,support.function.promise.ts,keyword.control.flow.ts",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d33682"
			}
		},
		{
			"scope": "constant.language.null.js",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#666666"
			}
		},
		{
			"scope": "constant.language.boolean,constant.language.boolean.false.ts,constant.language.boolean.true.ts",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#2c91af"
			}
		},
		{
			"scope": "support.type.object.console.js",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d33682"
			}
		},
		{
			"scope": "support.function.console.js",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#2c91af"
			}
		},
		{
			"scope": "variable.other.class.js",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d33682"
			}
		},
		{
			"name": "Tag attributes",
			"scope": "entity.other.attribute-name,entity.other.attribute-name.class.css",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#268bd2"
			}
		},
		{
			"scope": "support.type.property-name.scss,support.type.property-name.css",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#fe7d9d"
			}
		},
		{
			"scope": "support.constant.property-value.scss,support.constant.property-value.css",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#3C8F93"
			}
		},
		{
			"scope": "entity.name.tag.scss,support.constant.tag.css",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#dc322f"
			}
		},
		{
			"scope": "support.function.misc.scss",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#2c91af"
			}
		},
		{
			"scope": "variable.scss",
			"settings": {
				"foreground": "#00AFD6"
			}
		},
		{
			"scope": "variable.language.constructor",
			"settings": {
				"foreground": "#9966b8"
			}
		},
		{
			"settings": {
				"background": "#000c18",
				"foreground": "#6688cc"
			}
		},
		{
			"name": "Numbers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Built-in constants",
			"scope": "constant.language",
			"settings": {
				"foreground": "#f280d0"
			}
		},
		{
			"name": "User-defined constants",
			"scope": [
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#f280d0"
			}
		},
		{
			"name": "Variables",
			"scope": "variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Keywords",
			"scope": "keyword",
			"settings": {
				"foreground": "#225588"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "",
				"foreground": "#225588"
			}
		},
		{
			"name": "Storage types",
			"scope": "storage.type",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9966b8"
			}
		},
		{
			"scope": "storage.type.let",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ededed"
			}
		},
		{
			"name": "Class names",
			"scope": [
				"entity.name.class",
				"entity.name.type"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#49006A"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#ddbb88"
			}
		},
		{
			"name": "Function arguments",
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#2277ff"
			}
		},
		{
			"name": "Tag names",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "",
				"foreground": "#225588"
			}
		},
		{
			"name": "Library functions",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#9966b8"
			}
		},
		{
			"name": "diff: header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#E0EDDD"
			}
		},
		{
			"name": "diff: deleted",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#dc322f"
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#cb4b16"
			}
		},
		{
			"name": "diff: inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#219186"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#22aa44"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#22aa44"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#9966b8"
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"foreground": "#ddbb88"
			}
		}
	]
}