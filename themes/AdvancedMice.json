{
  "name": "AdvancedMice",
  "type": "light",
  "colors": {
    "editor.background": "#fcfcf7",
    "activityBar.background": "#e1dcc5",
    "editor.selectionBackground": "#ffced1",
    "editor.lineHighlightBackground": "#e1dcc5",
    "editorBracketMatch.background": "#69481E",
    "editorBracketMatch.border": "#7D1217",
    "editorGroupHeader.tabsBackground": "#D6E5DA",
    "editorGutter.background": "#f4f8f5",
    "editorLineNumber.foreground": "#C4BA8D",
    "scrollbarSlider.background": "#e1dcc5",
    "scrollbarSlider.hoverBackground": "#C4BA8D",
    "sideBar.background": "#D3CBA9",
    "statusBar.background": "#D3CBA9",
    "tab.activeForeground": "#D84A19",
    "tab.activeBackground": "#C4BA8D",
    "tab.inactiveForeground": "#D84A19",
    "tab.inactiveBackground": "#D3CBA9",
    "tab.border": "#D3CBA9"
  },
  "tokenColors": [
    {
      "name": "Language Constants",
      "scope": [
        "constant.language",
        "support.constant",
        "variable.language"
      ],
      "settings": {
        "foreground": "#9c171d",
        "fontStyle": "bold"
      }
    },
    {
      "SK": "const let for",
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#b43b3a"
      }
    },
    {
      "scope": "entity.name.function.method",
      "settings": {
        "foreground": "#cf1a52"
      }
    },
    {
      "SK": "function SK",
      "scope": "entity.name.function,support.function.dom.js",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#D84A19"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#90632A"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#cf1a52"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#A23534"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#9c171d"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#cf1a52"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#c44317",
        "fontStyle": "bold"
      }
    },
    {
      "name": "User-defined constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#cf1a52"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#835a26"
      }
    },
    {
      "scope": "variable.other.readwrite,variable.other.readwrite.js,variable.other.readwrite.ts,variable.other.readwrite.tsx,variable.other.object.js,punctuation.accessor.js",
      "settings": {
        "foreground": "#2b7c95"
      }
    },
    {
      "scope": "variable.other.property.js",
      "settings": {
        "foreground": "#226377"
      }
    },
    {
      "scope": "keyword.control.at-rule.import.scss,keyword.control.module.js,keyword.operator.accessor",
      "settings": {
        "foreground": "#835a26"
      }
    },
    {
      "SK": "IMPORT r FROM 'rambda'",
      "scope": "keyword.control.import.js,keyword.control.from.js,keyword.control.export.js,keyword.control.default.js,support.type.object.module.ts",
      "settings": {
        "foreground": "#835a26"
      }
    },
    {
      "scope": "string.template",
      "settings": {
        "foreground": "#E5326A"
      }
    },
    {
      "scope": "string.quoted.single.js,string.quoted.single.scss,string.quoted.single.attribute-value.scss,string.quoted.single.json,string.quoted.single.ts",
      "settings": {
        "foreground": "#E5326A"
      }
    },
    {
      "scope": "string.quoted.double.js,string.quoted.double.scss,string.quoted.double.attribute-value.scss,string.quoted.double.json,string.quoted.double.ts",
      "settings": {
        "foreground": "#E5326A"
      }
    },
    {
      "scope": "constant.other.object.key.js,meta.object-literal.key.js,support.type.property-name.json,meta.object-literal.key.ts,variable.other.property.ts",
      "settings": {
        "foreground": "#7D1217"
      }
    },
    {
      "scope": "support.class.builtin.js",
      "settings": {
        "foreground": "#BA174A"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#69481E"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#90632A"
      }
    },
    {
      "scope": "comment.line.double-slash,punctuation.definition.comment.scss,comment.line.scss",
      "settings": {
        "foreground": "#69481E"
      }
    },
    {
      "scope": "punctuation.definition.comment.html",
      "settings": {
        "foreground": "#2b7c95"
      }
    },
    {
      "scope": "comment.block.html",
      "settings": {
        "foreground": "#C4BA8D"
      }
    },
    {
      "scope": "variable.language.this,storage.type.function.js,keyword.control.flow.js,support.variable.dom.js,support.variable.property.dom.js,support.function.promise.ts,keyword.control.flow.ts",
      "settings": {
        "foreground": "#cf1a52"
      }
    },
    {
      "scope": "constant.language.null.js",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#BA174A"
      }
    },
    {
      "scope": "constant.language.boolean,constant.language.boolean.false.ts,constant.language.boolean.true.ts",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#902F2E"
      }
    },
    {
      "scope": "support.type.object.console.js",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "support.function.console.js",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "variable.other.class.js",
      "settings": {
        "foreground": "#BA174A"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name,entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "scope": "support.type.property-name.scss,support.type.property-name.css",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#cf1a52"
      }
    },
    {
      "scope": "support.constant.property-value.scss,support.constant.property-value.css",
      "settings": {
        "foreground": "#3C8F93"
      }
    },
    {
      "scope": "entity.name.tag.scss,support.constant.tag.css",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "support.function.misc.scss",
      "settings": {
        "foreground": "#2c91af"
      }
    },
    {
      "scope": "variable.scss",
      "settings": {
        "foreground": "#00AFD6"
      }
    },
    {
      "scope": "variable.language.constructor",
      "settings": {
        "foreground": "#A61542"
      }
    },
    {
      "settings": {
        "background": "#000c18",
        "foreground": "#6688cc"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#226377"
      }
    },
    {
      "scope": "storage.type.let",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "name": "Class names",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "fontStyle": "undereline",
        "foreground": "#8C151A"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#BB1C23"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#226377"
      }
    },
    {
      "name": "diff: header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#ddbb88"
      }
    }
  ]
}