{
  "name": "BraveNeighbour",
  "type": "light",
  "colors": {
    "editor.background": "#f9f6f2",
    "activityBar.background": "#eae9e8",
    "editor.selectionBackground": "#ffe0e0",
    "editor.lineHighlightBackground": "#eae9e8",
    "editorBracketMatch.background": "#AE2361",
    "editorBracketMatch.border": "#576881",
    "editorGroupHeader.tabsBackground": "#E9DFD0",
    "editorGutter.background": "#f9f6f2",
    "editorLineNumber.foreground": "#BEBAB7",
    "scrollbarSlider.background": "#eae9e8",
    "scrollbarSlider.hoverBackground": "#BEBAB7",
    "sideBar.background": "#D4D2D0",
    "tab.inactiveForeground": "#4E5F52",
    "tab.inactiveBackground": "#C2C4A7",
    "tab.activeForeground": "#4E5F52",
    "tab.activeBackground": "#D2D4BF",
    "tab.border": "#D2D4BF"
  },
  "tokenColors": [
    {
      "SK": "const let for",
      "name": "Storage types",
      "scope": "storage.type",
      "settings": {
        "foreground": "#df6e7a"
      }
    },
    {
      "scope": "entity.name.function.method",
      "settings": {
        "foreground": "#b55b5b"
      }
    },
    {
      "scope": "entity.name.function,support.function.dom.js",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#35b0b3"
      }
    },
    {
      "name": "Numbers",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#D94688"
      }
    },
    {
      "name": "Built-in constants",
      "scope": "constant.language",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#b55b5b"
      }
    },
    {
      "name": "Function arguments",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D95361"
      }
    },
    {
      "name": "Library functions",
      "scope": "support.function",
      "settings": {
        "foreground": "#35b0b3"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#b55b5b"
      }
    },
    {
      "name": "Keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#12486c"
      }
    },
    {
      "name": "User-defined constants",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#b55b5b"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#d5307a"
      }
    },
    {
      "scope": "variable.other.readwrite,variable.other.readwrite.js,variable.other.readwrite.ts,variable.other.readwrite.tsx,variable.other.object.js,punctuation.accessor.js",
      "settings": {
        "foreground": "#6f839e"
      }
    },
    {
      "scope": "variable.other.property.js",
      "settings": {
        "foreground": "#576881"
      }
    },
    {
      "scope": "keyword.control.at-rule.import.scss,keyword.control.module.js,keyword.operator.accessor",
      "settings": {
        "foreground": "#d5307a"
      }
    },
    {
      "SK": "IMPORT r FROM 'rambda'",
      "scope": "keyword.control.import.js,keyword.control.from.js,keyword.control.export.js,keyword.control.default.js,support.type.object.module.ts",
      "settings": {
        "foreground": "#d5307a"
      }
    },
    {
      "scope": "string.template",
      "settings": {
        "foreground": "#C68080"
      }
    },
    {
      "scope": "string.quoted.single.js,string.quoted.single.scss,string.quoted.single.attribute-value.scss,string.quoted.single.json,string.quoted.single.ts",
      "settings": {
        "foreground": "#C68080"
      }
    },
    {
      "scope": "string.quoted.double.js,string.quoted.double.scss,string.quoted.double.attribute-value.scss,string.quoted.double.json,string.quoted.double.ts",
      "settings": {
        "foreground": "#C68080"
      }
    },
    {
      "scope": "constant.other.object.key.js,meta.object-literal.key.js,support.type.property-name.json,meta.object-literal.key.ts,variable.other.property.ts",
      "settings": {
        "foreground": "#2A8D8F"
      }
    },
    {
      "scope": "support.class.builtin.js",
      "settings": {
        "foreground": "#A94C4C"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#AE2361"
      }
    },
    {
      "scope": "comment.block.documentation",
      "settings": {
        "foreground": "#D94688"
      }
    },
    {
      "scope": "comment.line.double-slash,punctuation.definition.comment.scss,comment.line.scss",
      "settings": {
        "foreground": "#AE2361"
      }
    },
    {
      "scope": "punctuation.definition.comment.html",
      "settings": {
        "foreground": "#6f839e"
      }
    },
    {
      "scope": "comment.block.html",
      "settings": {
        "foreground": "#BEBAB7"
      }
    },
    {
      "scope": "variable.language.this,storage.type.function.js,keyword.control.flow.js,support.variable.dom.js,support.variable.property.dom.js,support.function.promise.ts,keyword.control.flow.ts",
      "settings": {
        "foreground": "#b55b5b"
      }
    },
    {
      "scope": "constant.language.null.js",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#A94C4C"
      }
    },
    {
      "scope": "constant.language.boolean,constant.language.boolean.false.ts,constant.language.boolean.true.ts",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D33748"
      }
    },
    {
      "scope": "support.type.object.console.js",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "support.function.console.js",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "scope": "variable.other.class.js",
      "settings": {
        "foreground": "#A94C4C"
      }
    },
    {
      "name": "Tag attributes",
      "scope": "entity.other.attribute-name,entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#268bd2"
      }
    },
    {
      "scope": "support.type.property-name.scss,support.type.property-name.css",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b55b5b"
      }
    },
    {
      "scope": "support.constant.property-value.scss,support.constant.property-value.css",
      "settings": {
        "foreground": "#3C8F93"
      }
    },
    {
      "scope": "entity.name.tag.scss,support.constant.tag.css",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "support.function.misc.scss",
      "settings": {
        "foreground": "#2c91af"
      }
    },
    {
      "scope": "variable.scss",
      "settings": {
        "foreground": "#00AFD6"
      }
    },
    {
      "scope": "variable.language.constructor",
      "settings": {
        "foreground": "#964444"
      }
    },
    {
      "settings": {
        "background": "#000c18",
        "foreground": "#6688cc"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#576881"
      }
    },
    {
      "scope": "storage.type.let",
      "settings": {
        "foreground": "#d33682"
      }
    },
    {
      "name": "Class names",
      "scope": [
        "entity.name.class",
        "entity.name.type"
      ],
      "settings": {
        "fontStyle": "undereline",
        "foreground": "#309EA1"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#4CC7CA"
      }
    },
    {
      "name": "Tag names",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7F91A9"
      }
    },
    {
      "name": "diff: header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#E0EDDD"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#dc322f"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#cb4b16"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#219186"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#22aa44"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#9966b8"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "foreground": "#ddbb88"
      }
    }
  ]
}